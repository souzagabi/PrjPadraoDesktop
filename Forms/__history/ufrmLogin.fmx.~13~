  object FDQryEmpresa: TFDQuery
    ConnectionName = 'PRJ020_XML'
    SQL.Strings = (
      'SELECT PESSOA_ID, CODIGOACESSO'
      'FROM PRJ020029'
      'WHERE CODIGOACESSO = :EMPRESA')
    Left = 40
    Top = 176
    ParamData = <
      item
        Name = 'EMPRESA'
        DataType = ftString
        ParamType = ptInput
        Value = 'IOM001'
      end>
  end
