  object FDQryLogin: TFDQuery
    ConnectionName = 'PRJ020_XML'
    SQL.Strings = (
      
        'SELECT PRJ003.USUARIO_ID, PRJ003.SENHA, PRJ003.LOGIN, PRJ003.STA' +
        'TUS, PRJ012.PESSOA_ID, PRJ022.NOME, PRJ022.PROFISSAO, '
      
        '       PRJ022.CPF, PRJ027.LOCALIDADE, PRJ027.UF, PRJ031.NROORGAO' +
        'PROFISSAO        '
      'FROM PRJ020003 PRJ003'
      ''
      
        'INNER JOIN PRJ020012 PRJ012 ON PRJ012.USUARIO_ID = PRJ003.USUARI' +
        'O_ID AND PRJ012.PESSOA_ID = :PESSOA_ID'
      
        'INNER JOIN PRJ020022 PRJ022 ON PRJ022.PESSOA_ID = PRJ003.USUARIO' +
        '_ID'
      
        'LEFT JOIN PRJ020027 PRJ027 ON PRJ027.PESSOA_ID = PRJ022.PESSOA_I' +
        'D'
      
        'LEFT JOIN PRJ020031 PRJ031 ON PRJ031.COLABORADOR_ID = PRJ022.PES' +
        'SOA_ID'
      'WHERE PRJ003.LOGIN = :LOGIN')
    Left = 60
    Top = 80
    ParamData = <
      item
        Name = 'PESSOA_ID'
        DataType = ftInteger
        ParamType = ptInput
        Value = 2
      end
      item
        Name = 'LOGIN'
        DataType = ftString
        ParamType = ptInput
        Value = 'AGEU'
      end>
  end
  object FDPhysFBDriverLink1: TFDPhysFBDriverLink
